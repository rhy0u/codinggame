{
  "presets": [
    "babel-preset-react",
    [
      "babel-preset-env",
      {
        "modules": "commonjs",
        "loose": true,
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./src"]
      }
    ],
    "babel-plugin-styled-components",
    "babel-plugin-transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    "babel-plugin-syntax-dynamic-import"
  ]
}
